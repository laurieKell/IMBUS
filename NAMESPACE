# IMBUS Package Namespace

# Import from dependencies
importFrom(FLCore, FLStock, FLIndex, FLQuant, indicators.len)
importFrom(FLRebuild, jabba)
importFrom(FLSkill, roc2)
importFrom(zoo, na.locf)

# Export IMBUS-specific functions
export(
    # DATRAS functions
    get_datras_table,
    
    # ROC wrapper function
    roc,
    
    # IMBUSData constructor
    imbusData,
    
    # F and effort proxy methods
    calcFeff,
    calcFproxies
)

# Export S4 classes
exportClass(IMBUSData)

# Re-export functions from dependencies (imported above, now exported)
export(indicators.len)
export(jabba)
export(FLStock, FLIndex, FLQuant)
